OnChoiceTimeout{
	if RE_GREP(reference0,"\%(IgnoreChoiceTimeout)$")
		return
	if playingtetris&&running=='playingtetris'
		OnTetris.Timer
	elseif playingGobang&&running=='playingGobang'{
		if aiturn==1
			ComputerThink
		else
			OnGobangShow
	}
	elseif playRetroSnaker&&running=='playRetroSnaker'
		OnRetroSnakerTimer
	elseif readbook&&running=='readbook'
		OnReadBook
	elseif kikkastock&&running=='kikkastock'{
		if kikkastock==1
			OnKikkaStockMenu
		elseif kikkastock==2
			OnKikkaStockSelect
		elseif kikkastock==3
			OnKikkaStockFound
	}
	elseif playingplane
		OnShowBackground
	elseif infomode
		AntiFlicker(OpenMenu(menu,menuex))
	elseif mode == '1'
		"\1\s[%(specialmode != 'mealtime')10]…啧。"
	else{
		talkmemo = 'e_0'
		'\0\s[7]…无视吗？'
		'\0\s[0]…好的'
		if nurturance.Moeness>nurturance.Dependency{
			"\0\s[7]%(username)，你很忙吗，都没有听我说什么"
			'\0\s[7]有在听我说吗？心不在焉的哦'
			"\0\s[7]最讨厌%(username)这一点了，都不听人家说话"
			"\0\s[7]%(username)..？在干什么呢"
		}
	}
}
