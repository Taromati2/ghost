GETTYPE.to_string{
	case _argv[0]{
		when 1
			'整数'
		when 2
			'实数'
		when 3
			'字串'
		when 4
			'泛用阵列'
		when 5
			'散列表'
		others
			"内部错误（typeid：%(_argv[0])）"
	}
}
ValueTOstring{
	_type=_argv[_argc-1]
	_argv[_argc-1]=IARRAY
	if _argc > 2 || _type==4{
		_t='('
		foreach _argv;_i{
			_t+=ValueTOstring(_i,0)+','
		}
		_t+=')'
		RE_REPLACE(_t,'\,\)$',')')
	}
	elseif ISSTR(_argv[0]){
		if '"' !_in_ _argv[0]
			'"'+_argv[0]+'"'
		elseif "'" !_in_ _argv[0]
			"'"+_argv[0]+"'"
		else
			'"'+REPLACE(_argv[0],'"','""')+'"'
	}
	elseif GETTYPE(_argv[0])==5{
		'map'
	}
	elseif TOSTR(_argv[0])==''
		'NULL'
	else
		TOSTR(_argv[0])
}
