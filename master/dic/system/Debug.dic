		ansi_cyan
		--
		_t='('
		foreach _argv;_i{
			_t+=ValueTOstring.Coloured(_i,0)+ansi_cyan+','
		}
		_t+=')'
		RE_REPLACE(_t,'\,\)$',ansi_cyan+')')
		--
		ansi_reset
	}
	elseif ISSTR(_argv[0]){
		ansi_magenta
		--
		if '"' !_in_ _argv[0]
			'"'+_argv[0]+'"'
		elseif "'" !_in_ _argv[0]
			"'"+_argv[0]+"'"
		else
			'"'+REPLACE(_argv[0],'"','""')+'"'
		--
		ansi_reset
	}
	elseif GETTYPE(_argv[0])==5{
		ansi_green
		--
		'map'
		--
		ansi_reset
	}
	elseif TOSTR(_argv[0])==''
		ansi_white+'NULL'+ansi_reset
