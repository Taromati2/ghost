}
TextEndTo{//取得从文本右边指定位置到关键字的内容
	//示例：TextEndTo('a+b=1234','=',1) 的结果为123
	_s = _argv[0]
	_to = _argv[1]
	_end = _argv[2]
	_p=STRSTR(_s,_to,0)
	_l=STRLEN(_s)
