	_total=ARRAYSIZE(_arr)
	for _i=0;_i<_total;_i++{
		for _j=_i+1;_j<_total;_j++{
			while _arr[_j]==_arr[_i]{
				_arr[_j]=IARRAY
				_total=ARRAYSIZE(_arr)
			}
		}
	}
	_arr
}
