DMS{//小数点转换度分秒
	_exp=TOREAL(DEG(_argv[0]))
	_dms=0
	_d=TOINT(_exp)
	_t=(_exp-_d)*60
	_m=TOINT(_t)
	_s=(_t-_m)*60
	_dms=_d+'°'+_m+"'"+_s
	_dms
}
factorial{//阶乘
	_a=_argv[0]
	_b=1.0
	for _i=1;_i<=_a;_i++{
		_b*=_i
	}
	_b
}
sigma{//求和
	_a=TOREAL(_argv[0])//下界
	_b=TOREAL(_argv[1])//上界
	_s=0
	if _b+1>_a
		_s=(_a+_b)*(_b+1-_a)/2
	else
		_s=-1
	/*
	for _i=_a;_i<=_b;_i++{
		_s+=_i
	}
	*/
	_s
}
arrange{
	_a=TOREAL(_argv[0])
	_b=TOREAL(_argv[1])
	_s=1.0
	for _i=_b;_i>0;_i--{
		_s*=_a
		_a--
	}
	_s
}
combine{
	_a=TOREAL(_argv[0])
	_b=TOREAL(_argv[1])
	if _b*2>_a
		_b=_a-_b
	_s=1.0
	_w=1.0
	for _i=_b;_i>0;_i--{
		_s*=_a
		_a--
		_w*=_i
	}
	_s=_s/_w
	_s
}
TextBeginTo{//取得从文本左边指定位置到关键字的内容
