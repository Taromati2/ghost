Calculate{
	_op=_argv[0]
	if _op=='carry'{
		ChangCarry(_argv[1])
		return
	}
	if _op=='radio'
		radio=_argv[1]
	if _op=='key'
		inputkey(TOSTR(_argv[1]))
	if _op=='oper'
		inputkey(_argv[1])
	if _op=="D°M'S"{
		m_exp+=DmsInput(m_res)
	}
	if _op=='deg'{
		deg=1
		dms=0
	}
	if _op=='dms'{
		deg=0
		dms=1
	}
	if _op=='drg'{
		drg=_argv[1]
	}
	if _op=='back'
		BackSpace
	if _op=='ce'{
		endNumber=1
		m_exp=0
		m_res=0
		deg=0
		dms=0
	}
	if _op=='recordclear'
		m_resrecord=IARRAY

	_m_error=Error(m_exp)
	if _m_error==''
		_m_error=evaluate(m_exp)
	m_res=_m_error

	if dms
		m_res=DMS(evaluate(m_exp))
	if deg
		m_res=DEG(evaluate(m_exp))
	if drg==1
		m_res=DMS(evaluate(m_exp+'*180/π'))
	elseif drg==2
		m_res=DEG(evaluate(m_exp+'*π/180'))
	//m_res=DEG(evaluate(m_exp))/57.295779513
	if _op=='='{
		endNumber=1
